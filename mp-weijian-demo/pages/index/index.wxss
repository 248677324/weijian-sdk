/**index.wxss**/


.container {
  padding: 10rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}
 
.feeds_loading {
  position: fixed;
  top: 50%;
  left: 50%;

}

.container .list-half {
  width: 360rpx;
  box-sizing: border-box;
}
.list-half .card {

  margin-bottom: 10rpx;
  padding: 0;
  border-radius: 10px;
  overflow: hidden;
  background: #000;
}

.list-half .card image {
  width: 100%;
  display: block;
  /* height: 100%; */
}

.music_btn {
  position: fixed;
  right: 50rpx;
  bottom: 50rpx;
  z-index: 100;
  width: 120rpx;
  height: 120rpx;
  border-radius: 100%;
  /* background: url("./img/music.png");s */
  /* background: red; */
  overflow: hidden;
}

.music_btn  image{
  width: 100%;
  height: 100%;
  display: block;
}

.loading {
  /* height: 60rpx; */
  margin-top: 40rpx;
  margin-bottom: 40rpx;
}